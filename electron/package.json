{
  "name": "wenko",
  "productName": "wenko",
  "version": "1.0.0",
  "type": "module",
  "main": "main.cjs",
  "scripts": {
    "dev": "concurrently -k \"npm run vite\" \"npm run electron:dev\"",
    "vite": "vite",
    "electron:dev": "wait-on http://localhost:3000 && electronmon .",
    "start": "npm run build && electron .",
    "build": "vite build",
    "build:live2d": "cd live2d/live2d-widget && npm run build",
    "pack": "npm run build && electron-builder --dir",
    "dist": "npm run build && electron-builder"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/vite": "^4.1.18",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "electron-builder": "^26.0.12",
    "electronmon": "^2.0.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "wait-on": "^8.0.3"
  },
  "build": {
    "appId": "com.daijinru.wenko",
    "productName": "wenko",
    "files": [
      "**/*"
    ]
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "class-variance-authority": "^0.7.1",
    "classic-stylesheets": "^2.1.3",
    "clsx": "^2.1.1",
    "electron": "^38.1.0",
    "express": "^5.1.0",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwind-merge": "^3.4.0"
  }
}
